<?xml version="1.0"?>
<extension key="com.aghstrategies.idbsurvey" type="module">
  <file>idbsurvey</file>
  <name>Individual Donor Benchmark Survey</name>
  <description>Calculates donor data to compare to other nonprofits in the Individual Donor Benchmark Survey.</description>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org/extensions/individual-donor-benchmark-survey-report</url>
    <url desc="Documentation">https://civicrm.org/extensions/individual-donor-benchmark-survey-report</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>AGH Strategies</author>
    <email>info@aghstrategies.com</email>
  </maintainer>
  <releaseDate>2017-03-06</releaseDate>
  <version>2.1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.6</ver>
    <ver>4.7</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Idbsurvey</namespace>
    <format>22.05.2</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
</extension>
